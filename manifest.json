{
  "name": "Accessibility Colors",
  "version": "0.0.1",
  "description": "Use our color schemes to help you see the web!",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "manifest_version": 3,
  "author": "Team Zero",
  "action": {
    "default_popup": "extension.html",
    "default_title": "Accessibility Colors"
  },
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}