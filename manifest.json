{
  "manifest_version": 2,
  "name": "Accessibility Colors",
  "version": "0.0.1",
  "description": "Use our color schemes to help you see the web!",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js",
        "popup.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}